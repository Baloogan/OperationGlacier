@{
    ViewBag.Title = "Map";
}
@Styles.Render("~/Content/leaflet.css")
@Scripts.Render("~/Scripts/leaflet.js")
<div id="map" style="width: 100%; height: 1000px; background: black;"></div>
<style>
    .container {
        margin-left: 0px;
        margin-right: 0px;
        width: 100%;
        max-width: none;
        background: black;
    }
</style>
<script>
		var map = L.map('map').setView([67, -76], 3);
L.tileLayer('https://secure.baloogancampaign.com:8081/map/render2/{z}/{x}/{y}.png', {

    attribution: '',
			id: 'examples.map-i875mjb7',
maxZoom: 6,minZoom: 3}).addTo(map);

</script>
